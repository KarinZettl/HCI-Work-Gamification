{
  "expo": {
    "name": "HCI-Work-Gamification",
    "slug": "HCI-Work-Gamification",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins":[
      [
        "expo-font",
        {
          "fonts":[
            "./app/assets/fonts/Open_Sans/OpenSans-SemiBold.ttf",
            "./app/assets/fonts/Open_Sans/OpenSans-Regular.ttf",
            "./app/assets/fonts/Open_Sans/OpenSans-Light.ttf",
            "./app/assets/fonts/Open_Sans/OpenSans-Bold.ttf",
            "./app/assets/fonts/Montserrat/Montserrat-Regular.ttf",
            "./app/assets/fonts/Montserrat/Montserrat-Light.ttf",
            "./app/assets/fonts/Montserrat/Montserrat-Bold.ttf",
            "./app/assets/fonts/Montserrat/Montserrat-SemiBold.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
